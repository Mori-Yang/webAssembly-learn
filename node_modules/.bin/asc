#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/bin/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/bin/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/assemblyscript@0.27.36/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/bin/asc.js" "$@"
else
  exec node  "$basedir/../.pnpm/assemblyscript@0.27.36/node_modules/assemblyscript/bin/asc.js" "$@"
fi
