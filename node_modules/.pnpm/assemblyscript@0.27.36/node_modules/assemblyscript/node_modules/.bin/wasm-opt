#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules:/Volumes/APFS-ITGZ-1T/CodeSpace/personal/webAssembly-learn/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/wasm-opt" "$@"
else
  exec node  "$basedir/../../../../../binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/wasm-opt" "$@"
fi
